{
  "name": "semantic-release-demo",
  "version": "0.0.0-development",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "release": {
    "branch": "master",
    "repositoryUrl": "git@github.com:AbdellaToronto/semantic-release-demo.git",
    "plugins": [
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "semantic-release": "semantic-release"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.7",
    "semantic-release": "^15.13.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdellaToronto/semantic-release-demo.git"
  }
}
